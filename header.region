<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">
    {squarespace-headers}
    <link href='https://api.tiles.mapbox.com/mapbox-gl-js/v0.53.0/mapbox-gl.css' rel='stylesheet' />
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <header class="header">
      {.section website}
        <div class="branding">
          <a class="branding__link" href="/" data-content-field="site-title">
            <!-- TODO USE CONDITIONAL
              {.if logoImageUrl}
                <img class="branding__image" src="{logoImageUrl}?format=original" alt="{siteTitle}" />
              {.or}
              {.end}
            -->
              <img class="branding__image" src="/assets/mokuola-honua-logo.svg" alt="{siteTitle}" />
          </a>
        </div>
      {.end}

      <squarespace:navigation navigationId="primary-nav" template="navigation" />
    </header>
