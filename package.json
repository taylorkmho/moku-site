{
  "name": "npm-base-template",
  "version": "1.0.0",
  "description": "Squarespace Base Template using NPM",
  "author": "Squarespace, Inc.",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production squarespace assemble && NODE_ENV=production webpack --display-modules",
    "watch": "squarespace assemble --watch & webpack --watch --hide-modules",
    "server": "squarespace runserver",
    "authserver": "squarespace runserver --auth",
    "start": "squarespace setup && (npm run watch & npm run server)",
    "start:auth": "squarespace setup && (npm run watch & npm run authserver)",
    "start:deploy": "cd build && git init && git add . && git commit -m 'Updated site' && git push --force https://mokuola-honua.squarespace.com/template.git master && cd .. && squarespace setup && (npm run watch & npm run authserver)",
    "deploy": "cd build && git init && git add . && git commit -m 'Updated site' && git push --force https://mokuola-honua.squarespace.com/template.git master",
    "build:deploy": "npm run build && npm run deploy",
    "lint": "eslint .",
    "lint:css": "stylelint 'styles/**/*.css'",
    "prettier": "prettier --write 'styles/**/*'",
    "format": "prettier --write 'styles/**/*'"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:squarespace/base-template-npm.git"
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@squarespace/core": "^1.1.0",
    "@squarespace/server": "^1.1.2",
    "@typeform/embed": "^0.6.1",
    "autoprefixer": "^9.1.5",
    "axios": "^0.18.0",
    "cssnano": "^4.1.0",
    "mapbox-gl": "^0.53.0",
    "postcss-import": "^12.0.0",
    "postcss-preset-env": "^6.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "@squarespace/toolbelt": "^0.9.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-unicorn": "^6.0.1",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-custom-media": "^7.0.6",
    "postcss-extend-rule": "^2.0.0",
    "postcss-font-magician": "^2.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1"
  }
}
