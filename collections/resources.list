<div class="section-heading">
  <h1>{collection.title}</h1>
</div>

<div class="layout-container layout-container--intro">
  <squarespace:block-field id="resources-intro" label="Introduction" columns="1" />
</div>

<section class="layout-container">
  <div class="resources" data-vue-container="resources" data-url="{collection.fullUrl}">
    <article class="resource">
      <ul>
        {.repeated section items}
          <li>
            {.passthrough?}
              <a id="post-{id}" data-item-id="{id}" href="{sourceUrl}" target="_blank">
                {title}
              </a>
            {.or}
              <a id="post-{id}" data-item-id="{id}" href="{fullUrl}">
                {title}
              </a>
            {.end}
          </li>
        {.or}
          No content
        {.end}
    </article>
  </div>
</section>

{@|apply footer-cta.block}
